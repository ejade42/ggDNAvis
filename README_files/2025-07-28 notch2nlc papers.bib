@article{westenberger_essential_2020,
	title = {Essential phenotypes of \textit{{NOTCH2NLC}}-related repeat expansion disorder},
	volume = {143},
	issn = {0006-8950},
	url = {https://doi.org/10.1093/brain/awz404},
	doi = {10.1093/brain/awz404},
	abstract = {This scientific commentary refers to ‘Expansion of GGC repeat in human-specific NOTCH2NLC gene is associated with essential tremor’ by Sun et al. (doi: 10.1093/brain/awz372).The family of repeat expansion disorders currently comprises over 30 members, more than half of which were discovered in the 1990s (Fig. 1) (Bahlo et al., 2018; Paulson, 2018). The advent of second-generation sequencing allowed the genetic basis of many other human diseases to be unravelled over the next 15 years using high-throughput technologies. However, these technologies were confined to short-read sequencing and were inefficient at detecting structural variants, including repeat expansions. More recently, new methods have emerged for the detection of repeat expansions including third-generation sequencing that provides longer, continuous sequences. This has resulted in a renaissance of research into the genetic basis of repeat expansion disorders and the discovery of eight novel pathogenic repeats in the last 3 years (Fig. 1). Among these, expanded GGC repeats in the non-coding region at the very beginning of the NOTCH2NLC gene have been shown by three independent research groups to cause neuronal intranuclear inclusion disease (NIID) (Ishiura et al., 2019; Sone et al., 2019; Tian et al., 2019). NIID is a complex sporadic or dominantly inherited neurodegenerative disorder, presenting with a wide range of ages at onset (AAO; infancy to adulthood) and with various combinations of dementia, parkinsonism, cerebellar ataxia, and peripheral neuropathy. Neuroimaging and neuropathological hallmarks of NIID include high-intensity signals in diffusion-weighted imaging in the corticomedullary junction and eosinophilic hyaline intranuclear inclusions (seen in the nervous system, other tissues, and skin biopsies). In this issue of Brain, Sun and co-workers report a fourth discovery of expanded GGC repeats in NOTCH2NLC, this time in patients with essential tremor (Sun et al., 2020).},
	number = {1},
	urldate = {2024-03-22},
	journal = {Brain},
	author = {Westenberger, Ana and Klein, Christine},
	month = jan,
	year = {2020},
	pages = {5--8},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/IF2UHALS/Westenberger and Klein - 2020 - Essential phenotypes of NOTCH2NLC-related repeat e.pdf:application/pdf;Snapshot:/Users/evelyn/Zotero/storage/DQYEF24R/5689566.html:text/html},
}

@article{sugiyama_coexistence_2021,
	title = {Coexistence of neuronal intranuclear inclusion disease and amyotrophic lateral sclerosis: an autopsy case},
	volume = {21},
	shorttitle = {Coexistence of neuronal intranuclear inclusion disease and amyotrophic lateral sclerosis},
	doi = {10.1186/s12883-021-02306-5},
	abstract = {Background
Neuronal intranuclear inclusion disease (NIID) is a rare neurodegenerative disease. Pathologically, it is characterized by eosinophilic hyaline intranuclear inclusions in the cells of the visceral organs as well as central, peripheral, and autonomic nervous system cells. Recently, a GGC repeat expansion in the NOTCH2NLC gene has been identified as the etiopathological agent of NIID. Interestingly, this GGC repeat expansion was also reported in some patients with a clinical diagnosis of amyotrophic lateral sclerosis (ALS). However, there are no autopsy-confirmed cases of concurrent NIID and ALS.

Case presentation
A 60-year-old Taiwanese woman reported a four-month history of progressive weakness beginning in the right foot that spread to all four extremities. She was diagnosed with ALS because she met the revised El Escorial diagnostic criteria for definite ALS with upper and lower motor neuron involvement in the cervical, thoracic, and lumbosacral regions. She died of respiratory failure at 22 months from ALS onset, at the age of 62 years. Brain magnetic resonance imaging (MRI) revealed lesions in the medial part of the cerebellar hemisphere, right beside the vermis (paravermal lesions). The subclinical neuropathy, indicated by a nerve conduction study (NCS), prompted a potential diagnosis of NIID. Antemortem skin biopsy and autopsy confirmed the coexistence of pathology consistent with both ALS and NIID. We observed neither eccentric distribution of p62-positive intranuclear inclusions in the areas with abundant large motor neurons nor cytopathological coexistence of ALS and NIID pathology in motor neurons. This finding suggested that ALS and NIID developed independently in this patient.

Conclusions
We describe a case of concurrent NIID and ALS discovered during an autopsy. Abnormal brain MRI findings, including paravermal lesions, could indicate the coexistence of NIID even in patients with ALS showing characteristic clinical phenotypes.},
