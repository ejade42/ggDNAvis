	file = {Full Text PDF:/Users/evelyn/Zotero/storage/3XKVLUAG/Protic et al. - 2024 - Somatic Instability Leading to Mosaicism in Fragile X Syndrome and Associated Disorders Complex Mec.pdf:application/pdf},
}

@article{hwang_both_2022,
	title = {Both cis and trans-acting genetic factors drive somatic instability in female carriers of the \textit{{FMR1}} premutation},
