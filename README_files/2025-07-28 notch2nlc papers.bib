	pmid = {20186122},
	pmcid = {PMC2857464},
	pages = {1248--1261},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/NBAFEZNC/Sellier et al. - 2010 - Sam68 sequestration and partial loss of function are associated with splicing alterations in FXTAS p.pdf:application/pdf},
}

@article{zu_ran_2013,
