	keywords = {Humans, Male, Female, Adult, Genetic Testing, Intranuclear Inclusion Bodies, Magnetic Resonance Imaging, Middle Aged, Neurodegenerative Diseases, Receptor, Notch2, Brain, Cerebral Cortex},
	pages = {953--956},
}

