	pmcid = {PMC4384652},
	pages = {423--437},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/X8KMKT7N/Brettschneider et al. - 2014 - TDP-43 pathology and neuronal loss in amyotrophic .pdf:application/pdf},
}

