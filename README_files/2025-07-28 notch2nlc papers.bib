	pages = {637--644},
	file = {Yu et al. - 2022 - Oculopharyngodistal myopathy.pdf:/Users/evelyn/Zotero/storage/97VSHUZR/Yu et al. - 2022 - Oculopharyngodistal myopathy.pdf:application/pdf},
}

