	pages = {334},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/8XS5YI3V/Zhao et al. - 2021 - Neuronal intranuclear inclusion disease presented .pdf:application/pdf},
}

@article{cao_notch2nlc-related_2021,
	title = {\textit{{NOTCH2NLC}}-related repeat expansion disorders: an expanding group of neurodegenerative disorders},
	volume = {42},
	issn = {1590-3478},
	shorttitle = {{NOTCH2NLC}-related repeat expansion disorders},
	url = {https://doi.org/10.1007/s10072-021-05498-3},
	doi = {10.1007/s10072-021-05498-3},
	abstract = {The NOTCH2NLC gene 5′ untranslated region (UTR) GGC repeat expansion mutations were identified as a genetic contributor of neuronal intranuclear inclusion disease (NIID) in 2019. Since then, the number of reported cases with NOTCH2NLC GGC repeat expansion in Asian and European populations has increased rapidly, indicating that the expanded mutation not only leads to the onset or progression of the NIID, but also may play an important role in multiple progressive neurological disorders, including Parkinson’s disease, essential tremor, multiple system atrophy, Alzheimer’s disease, frontotemporal dementia, amyotrophic lateral sclerosis, leukoencephalopathy, and oculopharyngodistal myopathy type 3. Nevertheless, the underlying pathogenic mechanism of the NOTCH2NLC 5′ UTR region GGC repeat expansion in these disorders remains largely unknown. This review aims to present recent breakthroughs on this mutation and improve our knowledge of a newly defined spectrum of disease: NOTCH2NLC-related repeat expansion disorder.},
	language = {en},
	number = {10},
