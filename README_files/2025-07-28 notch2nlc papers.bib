	keywords = {Oxidative stress, Huntington's disease, DNA repair, Ataxia telangiectasia-mutated (ATM), Poly ADP-ribose polymerase (PARP), Spinocerebellar ataxia},
	pages = {948--956},
	file = {Full Text:/Users/evelyn/Zotero/storage/G4JVLK2Z/Maiuri et al. - 2019 - DNA Damage Repair in Huntington's Disease and Other Neurodegenerative Diseases.pdf:application/pdf;ScienceDirect Snapshot:/Users/evelyn/Zotero/storage/ZN68E6FV/S1878747923009613.html:text/html},
