	journal = {Human Molecular Genetics},
	author = {Goold, Robert and Flower, Michael and Moss, Davina Hensman and Medway, Chris and Wood-Kaczmar, Alison and Andre, Ralph and Farshim, Pamela and Bates, Gill P and Holmans, Peter and Jones, Lesley and Tabrizi, Sarah J},
