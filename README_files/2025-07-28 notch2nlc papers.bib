@article{redmond_microarray_2004,
	title = {Microarray {Transfection} {Analysis} of {Transcriptional} {Regulation} by {cAMP}-dependent {Protein} {Kinase}},
	volume = {3},
	issn = {1535-9476, 1535-9484},
