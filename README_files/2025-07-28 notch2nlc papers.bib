	author = {Furuta, Minori and Sato, Masayuki and Kasahara, Hiroo and Tsukagoshi, Setsuki and Hirayanagi, Kimitoshi and Fujita, Yukio and Takai, Eriko and Aihara, Yuko and Okamoto, Koichi and Ikeda, Yoshio},
