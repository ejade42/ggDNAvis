	keywords = {GGC repeat expansions, Neuronal intranuclear inclusion disease, NOTCH2NLC gene, Skin biopsy},
	pages = {647--658},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/I8NCPBEG/Cao et al. - 2022 - Expanding the clinical spectrum of adult-onset neu.pdf:application/pdf},
