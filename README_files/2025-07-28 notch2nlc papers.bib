	pmid = {20418358},
	note = {Company: Cold Spring Harbor Laboratory Press
Distributor: Cold Spring Harbor Laboratory Press
Institution: Cold Spring Harbor Laboratory Press
Label: Cold Spring Harbor Laboratory Press
Publisher: Cold Spring Harbor Lab},
	keywords = {gene regulation, accessibility, cis-element, post-transcriptional regulation, RNA-binding protein, secondary structure},
	pages = {1096--1107},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/THR5KLYG/Li et al. - 2010 - Predicting in vivo binding sites of RNA-binding proteins using mRNA secondary structure.pdf:application/pdf},
