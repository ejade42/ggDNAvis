	author = {Brettschneider, Johannes and Arai, Kimihito and Del Tredici, Kelly and Toledo, Jon B. and Robinson, John L. and Lee, Edward B. and Kuwabara, Satoshi and Shibuya, Kazumoto and Irwin, David J. and Fang, Lubin and Van Deerlin, Vivianna M. and Elman, Lauren and McCluskey, Leo and Ludolph, Albert C. and Lee, Virginia M.-Y. and Braak, Heiko and Trojanowski, John Q.},
	month = sep,
