@article{green_ran_2016,
	series = {{RNA} {Metabolism} in {Neurological} {Disease}},
	title = {{RAN} translation—{What} makes it run?},
	volume = {1647},
	issn = {0006-8993},
	url = {https://www.sciencedirect.com/science/article/pii/S0006899316301962},
	doi = {10.1016/j.brainres.2016.04.003},
	abstract = {Nucleotide-repeat expansions underlie a heterogeneous group of neurodegenerative and neuromuscular disorders for which there are currently no effective therapies. Recently, it was discovered that such repetitive RNA motifs can support translation initiation in the absence of an AUG start codon across a wide variety of sequence contexts, and that the products of these atypical translation initiation events contribute to neuronal toxicity. This review examines what we currently know and do not know about repeat associated non-AUG (RAN) translation in the context of established canonical and non-canonical mechanisms of translation initiation. We highlight recent findings related to RAN translation in three repeat expansion disorders: CGG repeats in fragile X-associated tremor ataxia syndrome (FXTAS), GGGGCC repeats in C9orf72 associated amyotrophic lateral sclerosis (ALS) and frontotemporal dementia (FTD) and CAG repeats in Huntington disease. These studies suggest that mechanistic differences may exist for RAN translation dependent on repeat type, repeat reading frame, and the surrounding sequence context, but that for at least some repeats, RAN translation retains a dependence on some of the canonical translational initiation machinery. This article is part of a Special Issue entitled SI:RNA Metabolism in Disease.},
	urldate = {2025-03-23},
	journal = {Brain Research},
	author = {Green, Katelyn M. and Linsalata, Alexander E. and Todd, Peter K.},
	month = sep,
	year = {2016},
	keywords = {Amyotrophic lateral sclerosis, Huntington disease, Fragile X-associated tremor ataxia syndrome, Frontotemporal dementia, Translation initiation},
	pages = {30--42},
	file = {Accepted Version:/Users/evelyn/Zotero/storage/AKJG7EN4/Green et al. - 2016 - RAN translation—What makes it run.pdf:application/pdf;ScienceDirect Snapshot:/Users/evelyn/Zotero/storage/MHRUXP5W/S0006899316301962.html:text/html},
}

