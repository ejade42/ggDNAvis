@article{roy_g_2009,
	title = {G {Clustering} {Is} {Important} for the {Initiation} of {Transcription}-{Induced} {R}-{Loops} {In} {Vitro}, whereas {High} {G} {Density} without {Clustering} {Is} {Sufficient} {Thereafter}},
