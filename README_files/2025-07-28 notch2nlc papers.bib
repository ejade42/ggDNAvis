	pmid = {37388545},
	pmcid = {PMC10300412},
	pages = {1168904},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/2WLMGRG5/Zhou et al. - 2023 - Characteristics of autonomic dysfunction in neuron.pdf:application/pdf},
}

