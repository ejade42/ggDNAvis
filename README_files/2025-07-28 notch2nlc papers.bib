	year = {2023},
	keywords = {Neuronal intranuclear inclusion disease, Leukoencephalopathy, Acute encephalopathy-like episode, Autonomic neuropathy, Cyclic vomiting, Esophageal achalasia, Neuropathy},
	pages = {70--76},
	file = {Miyamoto et al. - 2023 - First detailed case report of a pediatric patient .pdf:/Users/evelyn/Zotero/storage/27FNPEXI/Miyamoto et al. - 2023 - First detailed case report of a pediatric patient .pdf:application/pdf;ScienceDirect Snapshot:/Users/evelyn/Zotero/storage/4IUQSNIL/S0387760422001619.html:text/html},
}

