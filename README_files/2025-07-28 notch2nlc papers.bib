	urldate = {2024-06-05},
	journal = {Heliyon},
	author = {Okamura, Shun and Takahashi, Makoto and Abe, Keisuke and Inaba, Akira and Sone, Jun and Orimo, Satoshi},
	month = aug,
	year = {2020},
	pmid = {32817896},
	pmcid = {PMC7424193},
	pages = {e04675},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/3KLZ94MR/Okamura et al. - 2020 - A case of neuronal intranuclear inclusion disease .pdf:application/pdf},
}

