	keywords = {Neurological disorders, Movement disorders, Neurology},
	pages = {1--17},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/IB8PRTTW/Welton et al. - 2021 - Essential tremor.pdf:application/pdf},
