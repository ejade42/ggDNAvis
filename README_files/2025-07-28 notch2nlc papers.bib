	note = {Publisher: De Gruyter},
	keywords = {mRNA, tRNA, recoding, ribosome, translation fidelity, translocation},
	pages = {755--767},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/CSWCSBVI/Poulis et al. - 2023 - The many faces of ribosome translocation along the mRNA reading frame maintenance, ribosome framesh.pdf:application/pdf},
}

