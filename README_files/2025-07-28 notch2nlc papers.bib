	journal = {BMC Neurology},
	author = {Wang, Hongfen and Feng, Feng and Liu, Jiajin and Deng, Jianwen and Bai, Jiongming and Zhang, Wei and Wang, Luning and Xu, Baixuan and Huang, Xusheng},
