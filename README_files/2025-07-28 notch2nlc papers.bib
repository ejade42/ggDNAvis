	journal = {BMC medical genomics},
	author = {Yang, Xinzhuang and Zhang, Dingding and Shen, Si and Li, Pidong and Li, Mengjie and Niu, Jingwen and Ma, Dongrui and Xu, Dan and Li, Shuangjie and Guo, Xueyu and Wang, Zhen and Zhao, Yanhuan and Ren, Haitao and Ling, Chao and Wang, Yang and Fan, Yu and Shen, Jianxiong and Zhu, Yicheng and Wang, Depeng and Cui, Liying and Chen, Lin and Shi, Changhe and Dai, Yi},
