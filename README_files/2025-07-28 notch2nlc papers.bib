	journal = {Essays in Biochemistry},
	author = {Lamark, Trond and Svenning, Steingrim and Johansen, Terje},
	editor = {Lane, Jon D. and Korolchuk, Viktor I. and Murray, James T.},
	month = dec,
	year = {2017},
	pages = {609--624},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/QHVUNJ2H/Lamark et al. - 2017 - Regulation of selective autophagy the p62SQSTM1 .pdf:application/pdf;Snapshot:/Users/evelyn/Zotero/storage/4TB5M3R3/Regulation-of-selective-autophagy-the-p62-SQSTM1.html:text/html},
}

@article{sellier_translation_2017,
