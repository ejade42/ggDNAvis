	doi = {10.1093/hmg/9.3.439},
	abstract = {Myotonic dystrophy type 1 (DM1) is one of a growing number of inherited human diseases whose molecular basis has been implicated as the expansion of a trinucleotide DNA repeat. Expanded disease-associated alleles of \&gt;50 CTG repeats are unstable in both the germline and soma. Expansion of the unstable alleles over time and variation of the level of mutation between the somatic tissues of an individual are thought to account at least partially for the tissue specificity and progressive nature of the symptoms. We previously generated a number of transgenic mouse lines containing a large expanded CTG repeat tract that replicated a number of the features of unstable DNA in humans, including frequent sex-specific changes in allele length during intergenerational transmission. Small length change mutations were apparent in the somatic tissues of young mice in all of the lines generated, but the gross instability observed in human DM1 patients was not replicated. We now show that in one of the lines, Dmt-D, spectacular, expansion-biased, tissue-specific instability is observed in older mice. The highest levels of instability were detected in kidney with gains of \&gt;500 repeats, representing a tripling of allele length, in some cells. Mosaicism accumulated in an age-dependent manner, but the tissue specificity did not obviously correlate with cell turnover. Such gross somatic mosaicism was not observed in three other lines examined, further emphasizing a role for flanking DNA in modulating repeat stability.},
	number = {3},
	urldate = {2025-03-10},
	journal = {Human Molecular Genetics},
	author = {Fortune, M. Teresa and Vassilopoulos, Christos and Coolbaugh, Mary I. and Siciliano, Michael J. and Monckton, Darren G.},
	month = feb,
	year = {2000},
	pages = {439--445},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/83IZYT9A/Fortune et al. - 2000 - Dramatic, expansion-biased, age-dependent, tissue-specific somatic mosaicism in a transgenic mouse m.pdf:application/pdf;Snapshot:/Users/evelyn/Zotero/storage/XIZF6LZX/715093.html:text/html},
}

@article{kojak_somatic_2024,
	title = {Somatic and intergenerational {G4C2} hexanucleotide repeat instability in a human {C9orf72} knock-in mouse model},
	volume = {52},
	issn = {0305-1048},
	url = {https://doi.org/10.1093/nar/gkae250},
	doi = {10.1093/nar/gkae250},
	abstract = {Expansion of a G4C2 repeat in the C9orf72 gene is associated with familial Amyotrophic Lateral Sclerosis (ALS) and Frontotemporal Dementia (FTD). To investigate the underlying mechanisms of repeat instability, which occurs both somatically and intergenerationally, we created a novel mouse model of familial ALS/FTD that harbors 96 copies of G4C2 repeats at a humanized C9orf72 locus. In mouse embryonic stem cells, we observed two modes of repeat expansion. First, we noted minor increases in repeat length per expansion event, which was dependent on a mismatch repair pathway protein Msh2. Second, we found major increases in repeat length per event when a DNA double- or single-strand break (DSB/SSB) was artificially introduced proximal to the repeats, and which was dependent on the homology-directed repair (HDR) pathway. In mice, the first mode primarily drove somatic repeat expansion. Major changes in repeat length, including expansion, were observed when SSB was introduced in one-cell embryos, or intergenerationally without DSB/SSB introduction if G4C2 repeats exceeded 400 copies, although spontaneous HDR-mediated expansion has yet to be identified. These findings provide a novel strategy to model repeat expansion in a non-human genome and offer insights into the mechanism behind C9orf72 G4C2 repeat instability.},
	number = {10},
	urldate = {2025-03-10},
	journal = {Nucleic Acids Research},
	author = {Kojak, Nada and Kuno, Junko and Fittipaldi, Kristina E and Khan, Ambereen and Wenger, David and Glasser, Michael and Donnianni, Roberto A and Tang, Yajun and Zhang, Jade and Huling, Katie and Ally, Roxanne and Mujica, Alejandro O and Turner, Terrence and Magardino, Gina and Huang, Pei Yi and Kerk, Sze Yen and Droguett, Gustavo and Prissette, Marine and Rojas, Jose and Gomez, Teodoro and Gagliardi, Anthony and Hunt, Charleen and Rabinowitz, Jeremy S and Gong, Guochun and Poueymirou, William and Chiao, Eric and Zambrowicz, Brian and Siao, Chia-Jen and Kajimura, Daisuke},
