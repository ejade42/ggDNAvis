	urldate = {2024-06-06},
	journal = {Brain and Development},
	author = {Miyamoto, Yosuke and Okazaki, Tetsuya and Watanabe, Keisuke and Togawa, Masami and Adachi, Tadashi and Kato, Ayumi and Ochiai, Ryoya and Tamai, Chisato and Sone, Jun and Maegaki, Yoshihiro},
