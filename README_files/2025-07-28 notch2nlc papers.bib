	journal = {BMC Neurology},
	author = {Zhao, Danhua and Zhu, Sha and Xu, Qinlan and Deng, Jianwen and Wang, Zhaoxia and Liu, Xianzeng},
	month = sep,
	year = {2021},
	pmid = {34479498},
