	urldate = {2024-05-30},
	journal = {Neurology: Genetics},
	author = {Lee, Seungbok and Yoon, Jihoon G. and Hong, Juhyeon and Kim, Taekeun and Kim, Narae and Vandrovcova, Jana and Yau, Wai Yan and Cho, Jaeso and Kim, Sheehyun and Kim, Man Jin and Kim, Soo Yeon and Lee, Soon-Tae and Chu, Kon and Lee, Sang Kun and Kim, Han-Joon and Choi, Jungmin and Moon, Jangsup and Chae, Jong-Hee},
