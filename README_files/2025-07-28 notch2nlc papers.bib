	pmcid = {PMC9950388},
	pages = {1117243},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/NQB8JRBT/Fu et al. - 2023 - A case report of neuronal intranuclear inclusion d.pdf:application/pdf},
}

