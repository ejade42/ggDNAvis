	urldate = {2024-06-05},
	journal = {Annals of Clinical and Translational Neurology},
	author = {Chen, Hao and Lu, Likui and Wang, Bin and Cui, Guiyun and Wang, Xingqi and Wang, Yujing and Raza, Hafiz Khuram and Min, Yan and Li, Keke and Cui, Yingying and Miao, Zhigang and Wan, Bo and Sun, Miao and Xu, Xingshun},
	month = sep,
	year = {2020},
	pmid = {32931652},
	pmcid = {PMC7545592},
	pages = {1930--1941},
	file = {PubMed Central Full Text PDF:/Users/evelyn/Zotero/storage/CNN4RKJ8/Chen et al. - 2020 - Re‐defining the clinicopathological spectrum of ne.pdf:application/pdf},
}

