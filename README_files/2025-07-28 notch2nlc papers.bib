	year = {2022},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/nan.12787},
	keywords = {NOTCH2NLC, neuronal intranuclear inclusion disease, oculopharyngodistal myopathy, CGG repeat expansion, GIPC1, LRP12, oculopharyngeal muscular dystrophy},
