	author = {Ogasawara, Masashi and Eura, Nobuyuki and Iida, Aritoshi and Kumutpongpanich, Theerawat and Minami, Narihiro and Nonaka, Ikuya and Hayashi, Shinichiro and Noguchi, Satoru and Nishino, Ichizo},
