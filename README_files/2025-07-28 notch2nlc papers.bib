	author = {Wu, Wei and Yu, Jiaxi and Qian, Xiaojing and Wang, Xue and Xu, Yuanyuan and Wang, Zhaoxia and Deng, Jianwen},
	month = dec,
	year = {2022},
	keywords = {Alzheimer's disease, Neuropathology, Neuronal intranuclear inclusion disease, Human brain},
