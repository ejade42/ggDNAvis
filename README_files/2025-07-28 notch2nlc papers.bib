	title = {Assessing the \textit{{NOTCH2NLC}} {GGC} expansion in {European} patients with essential tremor},
	volume = {143},
	issn = {0006-8950},
	url = {https://doi.org/10.1093/brain/awaa291},
	doi = {10.1093/brain/awaa291},
	abstract = {The mean expansion size in controls was 20.32 (20.02, 20.63) and a median of 21 whereas the mean expansion size in cases was 20.60 (20.20, 21.10) with a median of 21 (Fig. 1). The mean confidence intervals were calculated using 1000 bootstraps and are reported as bias-corrected and accelerated bootstrap intervals (BCa). No cases or controls had pathogenic repeat sizes. Next, a two-sample Kolmogorov-Smirnov test was done, and it was found that the two probability distributions did not differ significantly (D = 0.52, P = 0.48), suggesting that they are comparable. Given that the pathogenic expansion frequency was 5–6\% in Chinese essential tremor cases, we expected to find ∼10 samples with a pathogenic expansion (Sun et al., 2020). However, none of the European essential tremor cases or control subjects had a large repeat. The differences may suggest that the pathogenic expansion is rarer in European populations and points towards an ancestry-specific repeat implicated in essential tremor. Furthermore, we failed to find any differences between case-control repeat size distributions, suggesting that both cohorts have similar shape and spread, with no evident positive right shift in case repeat distribution. Future studies could evaluate larger essential tremor cohorts and assess whether the repeat is expanded in essential tremor patients with European ancestry. In conclusion, our results do not support a significant association between NOTCH2NLC 5ʹ UTR expansions and essential tremor risk in European ancestry. Future studies should investigate the role of expansions in essential tremor as typical mutation searches have not had a high yield and anticipation is seen in some families.},
	number = {11},
	urldate = {2024-06-05},
	journal = {Brain},
	author = {Liao, Calwing and Akçimen, Fulya and Diez-Fairen, Monica and Houle, Gabrielle and Ross, Jay P and Schmilovich, Zoe and Spiegelman, Dan and Vuokila, Veikko and Catoire, Hélène and Meijer, Inge A and Pastor, Pau and Rajput, Alex and Dion, Patrick A and Rouleau, Guy A},
	month = nov,
	year = {2020},
	pages = {e89},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/944CZLN5/Liao et al. - 2020 - Assessing the NOTCH2NLC GGC expansion in European .pdf:application/pdf;Snapshot:/Users/evelyn/Zotero/storage/XGCSMLLM/5955876.html:text/html},
}

@article{sun_reply_2020,
	title = {Reply: {Assessing} the \textit{{NOTCH2NLC}} {GGC} expansion in {European} patients with essential tremor},
	volume = {143},
	issn = {0006-8950},
	shorttitle = {Reply},
	url = {https://doi.org/10.1093/brain/awaa292},
	doi = {10.1093/brain/awaa292},
	abstract = {We thank Liao et al. (2020) for their interest in our recent publication (Sun et al., 2020). We read their letter with great interest, which describes that abnormal GGC repeat expansion in the 5′ region of NOTCH2NLC gene is rare in European patients with essential tremor, in contrast to our results in Chinese essential tremor cases. To date, pathogenic GGC repeat expansion of NOTCH2NLC has been found in familial essential tremor patients from mainland China (Chen et al., 2020a; Sun et al., 2020) and sporadic essential tremor cases from Singapore (Ng et al., 2020), but not in the European essential tremor population (Yau et al., 2020a). Intriguingly, this finding is highly consistent with the screening results of NOTCH2NLC GGC repeat expansion in neuronal intranuclear inclusion disease (NIID) and related disorders, suggesting that NOTCH2NLC GGC repeat expansion may be more common in Asian populations (Deng et al., 2019; Ishiura et al., 2019; Okubo et al., 2019; Sone et al., 2019; Tian et al., 2019; Chen et al., 2020b; Fang et al., 2020; Jiao et al., 2020), especially in Chinese and Japanese; while the pathogenic expansion is rare in the European population (Yau et al., 2020b). This ethnic difference may be related to a genetic founder effect in East Asian populations, as the C9orf72 hexanucleotide (GGGGCC) repeat expansion rates in patients with clinically diagnosed amyotrophic lateral sclerosis (ALS) and frontotemporal dementia (FTD) in East Asian populations were much lower than that observed in Caucasian populations (Balendra and Isaacs, 2018).},
	number = {11},
	urldate = {2024-06-05},
	journal = {Brain},
	author = {Sun, Qi-Ying and Guo, Ji-Feng and Tang, Bei-Sha},
	month = nov,
	year = {2020},
	pages = {e90},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/EUCT62YU/Sun et al. - 2020 - Reply Assessing the NOTCH2NLC GGC expansion in Eu.pdf:application/pdf;Snapshot:/Users/evelyn/Zotero/storage/8CXGU5BL/5955877.html:text/html},
}

