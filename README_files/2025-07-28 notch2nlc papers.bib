@article{tawfik_health_2023,
	title = {Health inequity in genomic personalized medicine in underrepresented populations: a look at the current evidence},
	volume = {23},
	issn = {1438-7948},
	shorttitle = {Health inequity in genomic personalized medicine in underrepresented populations},
	url = {https://doi.org/10.1007/s10142-023-00979-4},
	doi = {10.1007/s10142-023-00979-4},
	abstract = {Improvements in sequencing technology coupled with dramatic declines in the cost of genome sequencing have led to a proportional growth in the size and number of genetic datasets since the release of the human genetic sequence by The Human Genome Project (HGP) international consortium. The HGP was undeniably a significant scientific success, a turning point in human genetics and the beginning of human genomics. This burst of genetic information has led to a greater understanding of disease pathology and the potential of employing this data to deliver more precise patient care. Hence, the recognition of high-penetrance disease-causing mutations which encode drivers of disease has made the management of most diseases more specific. Nonetheless, while genetic scores are becoming more extensively used, their application in the real world is expected to be limited due to the lack of diversity in the data used to construct them. Underrepresented populations, such as racial and ethnic minorities, low-income individuals, and those living in rural areas, often experience greater health disparities and worse health outcomes compared to the general population. These disparities are often the result of systemic barriers, such as poverty, discrimination, and limited access to healthcare. Addressing health inequity in underrepresented populations requires addressing the underlying social determinants of health and implementing policies and programs which promoted health equity and reduce disparities. This can include expanding access to affordable healthcare, addressing poverty and unemployment, and promoting policies that combat discrimination and racism.},
	language = {en},
	number = {1},
	urldate = {2024-04-07},
	journal = {Functional \& Integrative Genomics},
	author = {Tawfik, Sherouk M. and Elhosseiny, Aliaa A. and Galal, Aya A. and William, Martina B. and Qansuwa, Esraa and Elbaz, Rana M. and Salama, Mohamed},
	month = jan,
	year = {2023},
	keywords = {Genomic medicine, Health inequity, Personalized medicine, Underrepresented populations},
	pages = {54},
	file = {Full Text PDF:/Users/evelyn/Zotero/storage/TE7Y2F45/Tawfik et al. - 2023 - Health inequity in genomic personalized medicine i.pdf:application/pdf},
}

