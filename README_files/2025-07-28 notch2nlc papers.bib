}

@article{shelbourne_huntingtons_1999,
	title = {A {Huntington}'s {Disease} {CAG} {Expansion} at the {Murine} {Hdh} {Locus} {Is} {Unstable} and {Associated} with {Behavioural} {Abnormalities} in {Mice}},
